{
  "name": "killbill/killbill-client",
  "description": "Official PHP client to the Killbill HTTP API",
  "type": "library",
  "autoload": {
      "psr-4": {
          "Killbill\\Client\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Killbill\\Client\\": "test/"
